        "build:peerIdFactory": "esbuild ./modules/dep-peerId-factory.js --bundle --sourcemap --format=esm --target=chrome114 --define:global=window --keep-names --outfile=./src/this/modules/lib-peerId-factory.js",
        "build:pubsub": "esbuild ./modules/dep-pubsub.js --bundle --sourcemap --format=esm --target=chrome114 --define:global=window --keep-names --outfile=./src/this/modules/lib-pubsub.js",
        "build:stream": "esbuild ./modules/dep-stream.js --bundle --sourcemap --format=esm --target=chrome114 --define:global=window --keep-names --outfile=./src/this/modules/lib-stream.js",
        "build:cbor": "esbuild ./modules/dep-cbor.js --bundle --sourcemap --format=esm --target=chrome114 --define:global=window --keep-names --outfile=./src/this/modules/lib-cbor.js",
        "build:libp2p": "esbuild ./modules/dep-libp2p.js --bundle --sourcemap --format=esm --target=chrome114 --define:global=window --keep-names --outfile=./src/this/modules/lib-libp2p.js",
        "build:filter": "esbuild ./modules/dep-filters.js --bundle --sourcemap --format=esm --target=chrome114 --define:global=window --keep-names --outfile=./src/this/modules/lib-filters.js",
        "build:peer-store": "esbuild ./modules/dep-peer-store.js --bundle --sourcemap --format=esm --target=chrome114 --define:global=window --keep-names --outfile=./src/this/modules/lib-peer-store.js",
        "build:gossipsub": "esbuild ./modules/dep-gossipsub.js --bundle --sourcemap --format=esm --target=chrome114 --define:global=window --keep-names --outfile=./src/this/modules/lib-gossipsub.js",
        "build:pubsubPeerDiscovery": "esbuild ./modules/dep-pubsubPeerDiscovery.js --bundle --sourcemap --format=esm --target=chrome114 --define:global=window --keep-names --outfile=./src/this/modules/lib-pubsubPeerDiscovery.js",
        "build:identify": "esbuild ./modules/dep-identify.js --bundle --sourcemap --format=esm --target=chrome114 --define:global=window --keep-names --outfile=./src/this/modules/lib-identify.js",
        "build:logger": "esbuild ./modules/dep-logger.js --bundle --sourcemap --format=esm --target=chrome114 --define:global=window --keep-names --outfile=./src/this/modules/lib-logger.js",
        "Gulp  ===========================": "",
        "=== desktop ===": "",
        "px2vw:1920": "gulp px2vw --dir ./docs/css --rootWidthDesktop 1920 --viewportHeigth 1080 --media desktop",
        "vw2px:1920": "gulp vw2px  --dir ./docs/css --rootWidthDesktop 1920 --viewportHeigth 1080 --media desktop",
        "=== tablet ===": "",
        "px2vw:768": "gulp px2vw --dir ./docs/css --rootWidthDesktop 768 --viewportHeigth 899 --media tablet",
        "vw2px:768": "gulp vw2px  --dir ./docs/css --rootWidthDesktop 768 --viewportHeigth 899 --media tablet",
        "=== mobile ===": "",
        "px2vw:320": "gulp px2vw --dir ./docs/css --rootWidthDesktop 320 --viewportHeigth 767 --media mobile",
        "vw2px:797.27": "gulp vw2px --dir ./src/component --rootWidthDesktop 797.27 --viewportHeigth 797.27 --media desktop",
        "end  ===========================": ""
    },
    "keywords": [
        "webgpu",
        "webgl",
        "example",
        "seed",
        "types",
        "typescript"
    ],
    "service": {
        "publicUrl": "/"
    },
