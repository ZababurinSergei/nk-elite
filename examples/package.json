{
    "name": "@newkind/elite",
    "version": "0.1.0",
    "description": "FreeQueue example",
    "main": "app.js",
    "author": "Vyacheslav Zababurin && Zababurin Sergei",
    "license": "LGPL-2.1",
    "type": "module",
    "private": "true",
    "scripts": {
        "Server ===========================": "",
        "start": "node server.mjs",
        "dev": "nodemon server.mjs",
        "Bundler ===========================": "",
        "build": "npm run build:webrtc && npm run build:lottieWeb && npm run build:tweenjs && npm run build:stats && npm run build:solarSystemTwig && npm run build:THREE && npm run build:orbitControls && npm run build:solarSystem && npm run build:lokijs && npm run build:peerIdFactory && npm run build:stream && npm run build:pubsub && npm run build:libp2p && npm run build:filter && npm run build:peer-store && npm run build:pubsubPeerDiscovery && npm run build:gossipsub && npm run build:identify && npm run build:logger && npm run build:cbor",
        "build:tweenjs": "esbuild ./modules/dep-tween.js --bundle --sourcemap --format=esm --target=chrome114 --define:global=window --keep-names --outfile=./src/this/modules/lib-tween.js",
        "build:webrtc": "esbuild ./modules/dep-webrtc.js --bundle --sourcemap --format=esm --target=chrome114 --define:global=window --keep-names --outfile=./src/this/modules/lib-webrtc.js",
        "build:lottieWeb": "esbuild ./modules/dep-lottieWeb.js --bundle --sourcemap --format=esm --target=chrome114 --define:global=window --keep-names --outfile=./src/this/modules/lib-lottieWeb.js",
        "build:stats": "esbuild ./modules/dep-stats.js --bundle --sourcemap --format=esm --target=chrome114 --define:global=window --keep-names --outfile=./src/this/modules/lib-stats.js",
        "build:solarSystem": "esbuild ./modules/dep-solar-system.js --bundle --sourcemap --format=esm --target=chrome114 --define:global=window --keep-names --outfile=./src/this/modules/lib-solar-system.js",
        "build:orbitControls": "esbuild ./modules/dep-solar-system-orbit-controls.js --bundle --sourcemap --format=esm --target=chrome114 --define:global=window --keep-names --outfile=./src/this/modules/lib-solar-system-orbit-controls.js",
        "build:THREE": "esbuild ./modules/dep-THREE.js --bundle --sourcemap --format=esm --target=chrome114 --define:global=window --keep-names --outfile=./src/this/modules/lib-THREE.js",
        "build:lokijs": "esbuild ./modules/dep-lokijs.js --bundle --sourcemap --format=esm --target=chrome114 --define:global=window --keep-names --outfile=./src/this/modules/lib-lokijs.js",
